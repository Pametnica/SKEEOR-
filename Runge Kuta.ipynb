    "# Класа за следење и анализа на метрики поврзани со управувачкиот  сигнал u\n",
    "class MetricsWindow:\n",
    "    def __init__(self, N: int, dt: float):\n",
    "        self.N = max(2, int(N))  # Големина на прозорецот (минимум 2 вредности за да има смисла анализа)\n",
    "        self.dt = dt             # Временски чекор меѓу мерењата (sampling time)\n",
    "\n",
    "        # Историја на апсолутни вредности на управувачкиот сигнал u\n",
    "        self.u_hist: List[float] = []\n",
    "\n",
    "        # Историја на промени (разлики) меѓу тековна и претходна вредност на u\n",
    "        self.delta_hist: List[float] = []\n",
    "\n",
    "        # Историја на брзини (прва разлика поделена со време) — мера за динамика\n",
    "        self.v_hist: List[float] = []\n",
    "\n",
    "        # Историја на забрзувања (втора разлика) — мера за „нервозност“ на контролата\n",
    "        self.a_hist: List[float] = []\n",
    "\n",
    "        # Последна вредност на u — потребна за пресметка на промена и брзина\n",
    "        self.last_u = None\n",
    "\n",
    "        # Последна вредност на v — потребна за пресметка на забрзување\n",
    "        self.last_v = None\n",
    "\n",
    "    def push(self, u: float):\n",
    "        # Додава нова вредност u и ажурира ги сите метрики\n",
    "\n",
    "        mag = float(abs(u))  # Се зема апсолутната вредност на u за да се анализира интензитет, не насока\n",
    "        # Дали смееме така?????? Зошто со - не знаеме дали ќе имам навистина реално зголемување/ намалување.\n",
    "        # Да не добиеме огромни вреднсти?\n",
    "\n",
    "        if self.last_u is None:\n",
    "            # Ако нема претходна вредност, не може да се пресмета промена, брзина или забрзување\n",
    "            self.delta_hist.append(0.0)\n",
    "            self.v_hist.append(0.0)\n",
    "            self.a_hist.append(0.0)\n",
    "        else:\n",
    "            # Промена (разлика) меѓу тековна и претходна вредност\n",
    "            delta = abs(mag - self.last_u)\n",
    "            self.delta_hist.append(delta)\n",
    "\n",
    "            # Брзина на промена (извод): разлика поделена со време\n",
    "            v = (mag - self.last_u) / max(self.dt, 1e-12)\n",
    "            self.v_hist.append(v)\n",
    "\n",
    "            # Забрзување (втор извод): разлика на брзини поделена со време\n",
    "            if self.last_v is None:\n",
    "                a = 0.0\n",
    "            else:\n",
    "                a = (v - self.last_v) / max(self.dt, 1e-12)\n",
    "            self.a_hist.append(a)\n",
    "\n",
    "            # Ажурирање на последната брзина\n",
    "            self.last_v = v\n",
    "\n",
    "        # Ажурирање на последната вредност на u\n",
    "        self.last_u = mag\n",
    "\n",
    "        # Додавање на тековната вредност во историјата\n",
    "        self.u_hist.append(mag)\n",
    "\n",
    "        # Ограничување на должината на историјата според прозорецот N\n",
    "        if len(self.u_hist) > self.N: self.u_hist.pop(0)\n",
    "        if len(self.delta_hist) > self.N: self.delta_hist.pop(0)\n",
    "        if len(self.v_hist) > self.N: self.v_hist.pop(0)\n",
    "        if len(self.a_hist) > self.N: self.a_hist.pop(0)\n",
    "\n",
    "    def current_delta(self):\n",
    "        # Враќа последната промена на u — мера за моментална осцилација\n",
    "        return float(self.delta_hist[-1]) if self.delta_hist else 0.0\n",
    "\n",
    "    def delta_rms(self):\n",
    "        # RMS (Root Mean Square) на промените — мера за просечна амплитуда на варијации\n",
    "        a = np.asarray(self.delta_hist) if self.delta_hist else np.array([0.0])\n",
    "        return float(np.sqrt(np.mean(a**2)))\n",
    "\n",
    "    def delta_cum(self):\n",
    "        # Кумулативна сума на апсолутни промени — мера за вкупна активност на управувањето\n",
    "        a = np.asarray(self.delta_hist) if self.delta_hist else np.array([0.0])\n",
    "        return float(np.sum(np.abs(a)))\n",
    "\n",
    "    def delta_max(self):\n",
    "        # Најголема апсолутна промена — детектира екстремни скокови во управувањето\n",
    "        a = np.asarray(self.delta_hist) if self.delta_hist else np.array([0.0])\n",
    "        return float(np.max(np.abs(a)))\n",
    "\n",
    "    def delta_freq(self, thr: float):\n",
    "        # Фреквенција на промени над праг (thr) — мера за „немирност“ на управувачкиот сигнал\n",
    "        a = np.asarray(self.delta_hist) if self.delta_hist else np.array([0.0])\n",
    "        if a.size == 0: return 0.0\n",
    "        count = float(np.sum(a > thr))  # Броење колку пати промената надминува праг\n",
    "        return count / (len(a) * self.dt) if (len(a) * self.dt) > 0 else 0.0\n",
    "\n",
    "    def duty_cycle(self, eps: float):\n",
    "        # Процент од време кога u е „активен“ (над праг eps) — мера за колку често управувањето работи\n",
    "        # Се зема и за режимите на работа \n",
    "        u = np.asarray(self.u_hist) if self.u_hist else np.array([0.0])\n",
    "        if u.size == 0: return 0.0\n",
    "        return float(np.sum(np.abs(u) > eps)) / float(u.size)\n",
    "\n",
    "    def v_rms(self):\n",
    "        # RMS на брзината — мера за динамиката на контролата\n",
    "        a = np.asarray(self.v_hist) if self.v_hist else np.array([0.0])\n",
    "        return float(np.sqrt(np.mean(a**2)))\n",
    "\n",
    "    def a_rms(self):\n",
    "        # RMS на забрзувањето — мера за „нервозност“ или нестабилност (да не полетаме)\n",
    "        a = np.asarray(self.a_hist) if self.a_hist else np.array([0.0])\n",
    "        return float(np.sqrt(np.mean(a**2)))\n",
    "\n",
    "    def u_rms(self):\n",
    "        # RMS на самиот управувачки сигнал u — мера за неговата просечна енергија\n",
    "        # Се користи за проценка на енергетската ефикасност\n",
    "        a = np.asarray(self.u_hist) if self.u_hist else np.array([0.0])\n",
    "        return float(np.sqrt(np.mean(a**2)))\n"
