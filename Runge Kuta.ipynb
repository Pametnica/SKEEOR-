    "# Normalizer \n",
    "class Normalizer:\n",
    "    def __init__(self, cfg: Config):\n",
    "        self.cfg = cfg\n",
    "        # Иницијализација на базни вредности S за секоја метрика\n",
    "        # S служи како адаптивна скала за нормализација (EMA база)\n",
    "        self.S = {k: max(1e-6, 1.0) for k in cfg.default_weights.keys()}\n",
    "        self.S['e'] = 1e-3  # Посебна почетна вредност за грешка\n",
    "\n",
    "    def observe_and_update(self, key: str, v: float):\n",
    "        # https://www.investopedia.com/terms/e/ema.asp\n",
    "        # https://medium.com/@mburakbedir/understanding-exponential-moving-averages-ema-and-their\n",
    "        #-applications-in-python-eccb9518d729\n",
    "        # Ажурирање на базната вредност S[key] според новата вредност v\n",
    "        # Се користи експоненцијално подвижна средина (EMA)\n",
    "        v = abs(float(v))  # Се зема апсолутна вредност за стабилност\n",
    "        S_old = self.S.get(key, self.cfg.eps_S)  # Претходна база\n",
    "        # Избор на брзина на ажурирање:\n",
    "        # - Ако v > S_old → користи побрзо ажурирање (mu_up)\n",
    "        # - Ако v ≤ S_old → користи побавно ажурирање (mu_down)\n",
    "        mu = self.cfg.mu_up if v > S_old else self.cfg.mu_down\n",
    "        # EMA формула:\n",
    "        # S_new = (1 - mu) * S_old + mu * v\n",
    "        self.S[key] = max(self.cfg.eps_S, (1.0 - mu) * S_old + mu * v) # Овие вредности се користат за нормализација\n",
    "\n",
    "    def normalize(self, key: str, v: float):\n",
    "        # Нормализација на вредност v според тековната база S[key]\n",
    "        Sval = max(self.S.get(key, self.cfg.eps_S), self.cfg.eps_S)\n",
    "        # Нормализирана вредност (безединична):\n",
    "        # tilde_v = |v| / Sval\n",
    "        til = abs(v) / Sval\n",
    "        # Ограничување на максимумот за стабилност\n",
    "        return float(min(til, self.cfg.M_cap))\n",
    "\n",
