    "class StepAdapter:\n",
    "    def __init__(self, cfg: Config):\n",
    "        self.cfg = cfg              # Конфигурациски параметри (h_min, h_max, eta, lambda_M, safety итн.)\n",
    "        self.M_filt = 0.0           # Филтрирана вредност на метриката M (за стабилност)\n",
    "        self.h_last = None          # Последен прифатен чекор h (за мазнење)\n",
    "        self.prev_M = 0.0           # Претходна вредност на M (за ограничување на промени)\n",
    "\n",
    "    def classical_h_rk(self, h_old, e_est, x, x_high, p=4):\n",
    "        # Класична формула за адаптација на чекорот во RK45 според проценетата грешка e_est\n",
    "        # p = ред на методот ( апроксиација?) (Cash-Karp има p = 4)\n",
    "        tol_vec = self.cfg.tol_abs + self.cfg.tol_rel * np.maximum(np.abs(x), np.abs(x_high))\n",
    "        scale = np.linalg.norm(tol_vec, ord=2) + 1e-16  # нормализациски скалар за грешка\n",
    "        if e_est <= 0:\n",
    "            # Ако нема грешка, дозволи раст на чекорот (до максимум)\n",
    "            return float(min(h_old * self.cfg.r_base, self.cfg.h_max))\n",
    "        exponent = 1.0 / float(p + 1.0)\n",
    "        # Формула: h_new = h_old * ((safety * scale) / e_est)^(1/(p+1))\n",
    "        h_new = h_old * ((self.cfg.safety * scale) / (e_est + 1e-16))**exponent\n",
    "        return float(clamp(h_new, self.cfg.h_min, self.cfg.h_max))  # ограничување во дозволени граници\n",
    "\n",
    "    def rational_map(self, h_old, M):\n",
    "        # h_new = (K_map / (1 + M)) * h_old\n",
    "        # Кога M расте → чекорот се намалува\n",
    "        return float(clamp((self.cfg.K_map / (1.0 + M + 1e-16)) * h_old, self.cfg.h_min, self.cfg.h_max))\n",
    "\n",
    "    def exp_map(self, h_old, M):\n",
    "        # h_new = h_old * exp(-eta * M)\n",
    "        # Кога M расте → експоненцијално се намалува чекорот, агресија\n",
    "        return float(clamp(h_old * math.exp(-self.cfg.eta * M), self.cfg.h_min, self.cfg.h_max))\n",
    "\n",
    "    def lowpass_M(self, M_new):\n",
    "        # Филтрирање на M со low-pass филтер (експоненцијално мазнење)\n",
    "        # M_filt = (1 - lam) * M_prev + lam * M_new\n",
    "        lam = max(0.0, min(1.0, self.cfg.lambda_M))  # λ ∈ [0, 1]\n",
    "        self.M_filt = (1.0 - lam) * self.M_filt + lam * M_new\n",
    "        return self.M_filt\n",
    "\n",
    "    def adaptive_rate_limit(self, h_candidate, h_old, M):\n",
    "        # Ограничување на брзината на промена на чекорот h\n",
    "        # dM = апсолутна промена на M\n",
    "        dM = abs(M - self.prev_M)\n",
    "        # Долна граница: h не смее да падне премногу брзо\n",
    "        lower = max(self.cfg.h_min, h_old * (1.0 / (1.0 + 2.0 * self.M_filt)))\n",
    "        # Горна граница: h не смее да скокне премногу брзо\n",
    "        upper = min(self.cfg.h_max, h_old * (1.0 + 0.5 / (1.0 + 10.0 * dM)))\n",
    "        self.prev_M = M  # ажурирање на претходната вредност\n",
    "        return float(clamp(h_candidate, lower, upper))  # враќа ограничен чекор\n",
    "\n",
    "    def adapt(self, h_old, e_est, x, x_high, M):\n",
    "        # Главната функција за адаптација на чекорот h\n",
    "        # Врши комбинирана проценка од три методи: класична, рационална и експоненцијална (најдени на GOOGLE)\n",
    "        h_rk = self.classical_h_rk(h_old, e_est, x, x_high)  # чекор според грешка\n",
    "        h1 = self.rational_map(h_old, M)                     # чекор според рационална мапа\n",
    "        h2 = self.exp_map(h_old, M)                          # чекор според експоненцијална мапа\n",
    "        h_cand = min(h_rk, h1, h2)                           # земи најпараноичниот предлог\n",
    "        h_cand = self.adaptive_rate_limit(h_cand, h_old, M) # ограничи промена на чекорот\n",
    "\n",
    "        # Пеглање на чекорот со експоненцијална мешавина (за да нема скокови)\n",
    "        if self.h_last is None:\n",
    "            self.h_last = h_cand\n",
    "        else:\n",
    "            alpha = 0.2  # тежина за новиот чекор\n",
    "            self.h_last = alpha * h_cand + (1 - alpha) * self.h_last\n",
    "\n",
    "        return float(self.h_last), h_rk  # враќа финален чекор и класичен чекор за споредба\n"
   ]
